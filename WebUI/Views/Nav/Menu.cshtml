@model IEnumerable<string>

@Html.ActionLink("Home", "List","Photos", null, new { @class = "btn btn-block btn-default btn-lg"})

@foreach(var link in Model)
{
    @Html.RouteLink(link, new
    {

        controller = "Photos",
        action = "List",
        colortype = link,
        page = 1

    }, new
    {
        @class = "btn btn-block btn-default "
        +(link == ViewBag.SelectedColorType ? "btn-primary" : "")
    })

}